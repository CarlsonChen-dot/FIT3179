{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 400,
    "title": "COVID-19 Cases per Country (13 Oct 2020)",
    "data": {
        "url": "https://raw.githubusercontent.com/CarlsonChen-dot/FIT3179_DV2/main/FIT3179_DV2/data/uk_universities.csv"
    },
    "mark": "circle",
    "encoding": {
        "x": {
            "field": "World_rank",
            "type": "quantitative",
            "title": "World Rank"
        },
        "y": {
            "field": "CWUR_score",
            "type": "quantitative"
        },
        "color": {
            "field": "Region",
            "type": "nominal"
        },
        "size": {
            "field": "International_students",
            "type": "quantitative"
        }
    }
}