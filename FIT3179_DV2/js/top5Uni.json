{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Top 5 Universities.",
    "data": {
      "url": "https://raw.githubusercontent.com/CarlsonChen-dot/FIT3179/main/FIT3179_DV2/data/uk_universities.csv"
    },
    "transform": [
      {
        "window": [
          {
            "op": "rank",
            "as": "rank"
          }
        ],
        "sort": [
          {
            "field": "UK_rank",
            "order": "ascending"
          }
        ]
      },
      {
        "filter": "datum.UK_rank <= 5"
      }
    ],
    "mark": "bar",
    "encoding": {
      "x": {
        "field": "UK_rank",
        "type": "quantitative",
        "title": "UK rank"
      },
      "y": {
        "field": "University_name",
        "type": "nominal",
        "sort": {"field": "rank", "order": "ascending"}
      },
      "color": {"value": "green"}
    }
  }